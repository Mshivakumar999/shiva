{
	"name": "dataflow2",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText2",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Country as string,",
				"          Population as string,",
				"          YearlyChange as string,",
				"          NetChange as string,",
				"          Density as string,",
				"          LandArea as string,",
				"          Migrants as string,",
				"          {Fert.Rate} as string,",
				"          {Med.Age} as string,",
				"          {UrbanPop%} as string,",
				"          WorldShare as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 derive(Population = greater(toInteger(LandArea),20000)) ~> derivedColumn1"
			]
		}
	}
}